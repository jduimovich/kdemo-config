
if [ -z "$DOCKER_USER" ]
then
echo "\n"No DOCKER_USER set - using default jduimovich, this will pull images from that user.  "\n" 
export DOCKER_USER=jduimovich
else 
echo "\n " DOCKER_USER is set to $DOCKER_USER "\n"
fi
 
export DEMO_V1=$DOCKER_USER/kdemo-app-h11-m37-s24
export DEMO_V2=$DOCKER_USER/kdemo-app-h18-m20-s39
